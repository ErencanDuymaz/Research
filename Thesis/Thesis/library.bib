file = {:C$\backslash$:/Users/Eren/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Wang et al. - 2015 - Control of PMSG-Based Wind Turbines for System Inertial Response and Power Oscillation Damping(2).pdf:pdf},
