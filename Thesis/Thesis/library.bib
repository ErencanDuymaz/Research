file = {:C$\backslash$:/Users/Eren/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Eto et al. - 2010 - Use of Frequency Response Metrics to Assess the Planning and Operating Requirements for Reliable Integration of Vari.pdf:pdf},
keywords = {Use of Frequency Response Metrics to Assess the Pl},
number = {December 2010},
pages = {LBNL--4142E},
title = {{Use of Frequency Response Metrics to Assess the Planning and Operating Requirements for Reliable Integration of Variable Renewable Generation}},
url = {http://certs.lbl.gov/pdf/lbnl-4142e.pdf},
year = {2010}
}
