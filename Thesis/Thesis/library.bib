title = {{Evaluation of power system flexibility}},
volume = {27},
year = {2012}
}
@article{Haddad2016,
abstract = {In this paper, the motor current/voltage signature analysis and linear$\backslash$ndiscriminant analysis (LDA) are evaluated with respect to the accuracy$\backslash$nto detect the status of permanent magnet synchronous machines (PMSMs)$\backslash$nwhether it is healthy or faulted, determine the type of that fault, and$\backslash$nestimate the severity in the case of static eccentricity or turn-to-turn$\backslash$nshort-circuit fault. Three types of faults are discussed: static$\backslash$neccentricity, turn-to-turn short circuit, and partial demagnetization$\backslash$nfault. Two-dimensional finite element analysis (FEA) is used to model$\backslash$nand simulate the machine under healthy and faulted conditions. Fast$\backslash$nFourier transform is applied to the phase voltage or current signals to$\backslash$nobtain the frequency spectrum. A combination of the amplitude of the$\backslash$nharmonics of the stator voltage or current signals are used as detailed$\backslash$nfeatures for the classifier for fault detection. LDA is chosen as a$\backslash$nclassification method for both detecting the fault and estimating its$\backslash$nseverity. Two different winding types of PMSMs are tested: a$\backslash$nconcentrated and a distributed winding machine. To validate the$\backslash$nsimulation results, experiments at different operational points are$\backslash$ncarried out and the results are compared with the sFEA.},
