journal = {Ieee Transactions on Power Electronics},
keywords = {cascaded photovoltaic (pv) system,generator,h-bridge converter,motor drive,multilevel inverter,power-voltage distribution,reactive power compensation,statcom,topologies,unsymmetrical active power,voltage},
