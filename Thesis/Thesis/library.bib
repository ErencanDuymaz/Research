@article{Ford1993,
abstract = {The conventional XRAM (MARX spelled backwards) current multiplier$\backslash$ncircuit is similar in concept to the better known MARX voltage multiplier$\backslash$ncircuit. In the MARX circuit, capacitors charged in parallel from$\backslash$na voltage source are switched to discharge in series producing an$\backslash$noutput voltage approximately equal to the sum of individual capacitor$\backslash$nvoltages. In the XRAM circuit, inductors energized in series from$\backslash$na current source (battery, homopolar generator, other) are switched$\backslash$nto discharge in parallel, producing an output current which is the$\backslash$nsum of the individual inductor currents. In high energy pulsed power$\backslash$nsystems these multiplier techniques simplify primary power design$\backslash$nat the expense of more complex switching. The hybrid XRAM circuit$\backslash$ndescribed in this paper minimizes switching requirements while retaining$\backslash$nthe positive benefits of the conventional XRAM circuit. It also provides$\backslash$na smooth rising current waveform which is useful for EM and ET gun$\backslash$nresearch. The tradeoff for these benefits is that, during switching,$\backslash$napproximately 50{\%} of total system energy is stored in a capacitor$\backslash$nbank of simple design. In addition to simplified switching, the hybrid$\backslash$nconcept offers other advantages over either an all capacitive or$\backslash$nall inductive system. As a laboratory power supply at 2-3 MA current$\backslash$nand 10-20 MJ stored energy, it doubles energy which would be available$\backslash$nfrom a capacitive system while offering the current ''stiffness'$\backslash$navailable from an inductive system. Less obvious but potentially$\backslash$nof greater interest, the concept appears to scale to high energy$\backslash$n(10's to 100's of MJ) for either a distributed energy or multi shot$\backslash$npower supply. This paper outlines two XRAM circuit concepts, and$\backslash$ndiscusses the distributed energy and multi-shot aspects of the hybrid$\backslash$nconcept. It also describes a more detailed study of a battery powered$\backslash$nhybrid XRAM current doubler design being considered for use as a$\backslash$ngeneral purpose laboratory inductive energy supply, operating at$\backslash$nup to 2.5 MA with as much as 10 MJ of electrical energy stored. The$\backslash$ncircuit described is modeled using known values from existing circuit$\backslash$ncomponents. Resulting waveforms are shown and benefits derived from$\backslash$nthe use of this circuit are compared to those from a conventional$\backslash$nXRAM current doubler as well as from a direct, inductive store, system$\backslash$ndesign.},
author = {Ford, Richard D. and Hudson, Roger D. and Klug, Russell T.},
doi = {10.1109/20.195706},
file = {::},
isbn = {0018-9464},
issn = {19410069},
journal = {IEEE Transactions on Magnetics},
number = {1},
pages = {949--953},
title = {{Novel hybrid XRAM current multiplier}},
volume = {29},
year = {1993}
}
