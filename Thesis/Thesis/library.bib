file = {::},
issn = {00933813},
journal = {IEEE Transactions on Plasma Science},
keywords = {Armature,payload acceleration,railgun},
number = {7},
pages = {1234--1238},
title = {{Developing a Launch Package for the PEGASUS Launcher}},
volume = {45},
year = {2017}
}
