isbn = {0018-9499},
issn = {00189499},
journal = {IEEE Transactions on Nuclear Science},
keywords = {Annealing,Blue light-emitting diodes,Displacement energy,Indium gallium nitride,Relativistic electron irradiation,Room temperature irradiation},
number = {6},
pages = {2733--2738},
title = {{Radiation hardness of gallium nitride}},
volume = {49 I},
year = {2002}
}
