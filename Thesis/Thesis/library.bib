pages = {418--421},
title = {{Cracking and dominant stresses in the throat region of C-shaped solid armatures}},
volume = {43},
year = {2007}
}
@article{Gharakhany2009,
abstract = {This paper proposes using a Z-source inverter for grid interconnected photovoltaic (PV) system (Z-SPV). In this paper, besides boosting PV array voltage to required value, maximum power point tracking, and interfacing to the grid, a Z-source based grid connected photovoltaic system is used as a reactive power compensator (RPC). Its main feature is the capability to compensate the reactive currents drawn by grid or nonlinear loads while simultaneously injecting into the grid the maximum power available from the cells. Injecting active power and compensation of the reactive current of the system are realized simultaneously at daylight. When the insolation is weak or the PV modules are operative at night, the RPC feature of PV system can still be used to improve the utilization factor of the system. The simulation results validate the feasibility of the method.},
