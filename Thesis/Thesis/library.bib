keywords = {3-D model,finite-element method,multishot,open barrel,railgun,transient dynamics},
number = {5},
pages = {1510--1515},
title = {{Structural mechanics of railguns with open barrels and elastic supports: The influence of multishot operation}},
volume = {43},
year = {2015}
}
