journal = {IEEE Transactions on Power Electronics},
keywords = {Bayes procedures,Electrolytic capacitors,Fault diagnosis,Monte Carlo methods,Reliability modeling},
title = {{Particle Filter-Based Prognostics for an Electrolytic Capacitor Working in Variable Operating Conditions}},
year = {2016}
}
