journal = {IEEE Trans. Magn.},
keywords = {Contact resistivity,Sliding electrical contact (SEC)},
number = {1},
pages = {313--318},
title = {{Effects of pressure-dependent contact resistivity on contact interfacial conditions}},
volume = {45},
year = {2009}
}
