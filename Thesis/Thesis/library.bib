journal = {2nd International Symposium on Power Electronics for Distributed Generation Systems, PEDG 2010},
keywords = {Current controller,LCL-filter,Parameter design,Voltage source converter},
pages = {490--494},
title = {{Design of current-controller with PR-regulator for LCL-filter based grid-connected converter}},
