title = {{Efficiency and Scaling of Constant Inductance Gradient DC Electromagnetic Launchers}},
url = {http://ieeexplore.ieee.org/lpdocs/epic03/wrapper.htm?arnumber=1661948},
volume = {42},
year = {2006}
}
