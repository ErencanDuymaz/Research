isbn = {1949-3029 VO  - 7},
issn = {19493029},
journal = {IEEE Transactions on Sustainable Energy},
keywords = {Cluster voltage controls,HVDC,offshore wind power plants,power loss,reactive power dispatch,voltage ride through},
number = {4},
pages = {1592--1601},
title = {{Coordinated Voltage Control in Offshore HVDC Connected Cluster of Wind Power Plants}},
volume = {7},
