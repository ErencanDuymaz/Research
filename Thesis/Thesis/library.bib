@article{Pickering2006a,
abstract = {A drive based on a matrix power converter for a 30 kW totally enclosed, fan cooled induction motor was developed to be fully integrated within the spatial envelope of the motor, located inside the end cover at the fan end of the machine. This paper describes the thermal design of the motor to dissipate the losses from the drive, which were approximately a third of those from the motor. The power electronic components were mounted on the inside of a new end cover which was equipped with an extensive array of fins cooled by air from the external fan. The electronic components were insulated from the air in the end region of the motor. A heat sink plate was installed within the motor behind the end cover to conduct the heat from the motor that would otherwise have been dissipated through the end cover. Computational fluid dynamics (CFD) was used to model the ventilation and heat transfer from the new end cover and to develop improved fan geometries. It was concluded that CFD can be used effectively to design motor cooling systems. An improved fan geometry was designed to keep the power electronics cool under high ambient temperatures with the motor running at full load},
author = {Pickering, S. and Thovex, F. and Wheeler, P. and Bradley, K.},
doi = {10.1109/IECON.2006.348109},
file = {::},
isbn = {1-4244-0390-1},
issn = {1553-572X},
journal = {IECON 2006 - 32nd Annual Conference on IEEE Industrial Electronics},
pages = {4794--4799},
title = {{Thermal Design of an Integrated Motor Drive}},
year = {2006}
}
@article{Karamanakos2014,
abstract = {Direct model predictive control (MPC) strategies that achieve long prediction horizons with a modest computational complexity are reviewed in this article, focusing on power electronics applications. In many MPC problems, a long prediction horizon is required to ensure an adequate closed-loop performance in steady state and to avoid stability issues. However, the computational effort of solving the optimization problem underlying MPC problems with long prediction horizons is often very great, making the implementation of such schemes in real time a difficult and challenging task. To overcome this difficulty, three established methodologies are surveyed that yield long prediction horizons with a modest computational burden. Case studies are investigated to substantiate the merits of these schemes. More specifically, for dc?dc boost converters, a move blocking strategy is reviewed, and for ac medium-voltage (MV) drives, both an extrapolation and an event-based horizon strategy are examined.},
author = {Karamanakos, Petros and Geyer, Tobias and Oikonomou, Nikolaos and Kieferndorf, Frederick D. and Manias, Stefanos},
doi = {10.1109/MIE.2013.2290474},
file = {::},
isbn = {1932-4529 VO - 8},
issn = {19324529},
journal = {IEEE Industrial Electronics Magazine},
number = {1},
pages = {32--43},
title = {{Direct model predictive control: A review of strategies that achieve long prediction intervals for power electronics}},
volume = {8},
year = {2014}
}
@article{DeLillo2010,
abstract = {This paper describes an experimental tool to evaluate and support the development of fault-tolerant machines designed for aerospace motor drives. Aerospace applications involve essentially safety-critical systems which should be able to overcome hardware or software faults and therefore need to be fault tolerant. A way of achieving this is to introduce variable degrees of redundancy into the system by duplicating one or all of the operations within the system itself. Looking at motor drives, multiphase machines, such as multiphase brushless dc machines, are considered to be good candidates in the design of fault-tolerant aerospace motor drives. This paper introduces a multiphase two-level inverter using a flexible and reliable field-programmable gate-array/digital-signal-processor controller for data acquisition, motor control, and fault monitoring to study the fault tolerance of such systems.},
author = {{De Lillo}, Liliana and Empringham, Lee and Wheeler, Pat W. and Khwan-On, Sudarat and Gerada, Chris and Othman, M. Nazri and Huang, Xiaoyan},
doi = {10.1109/TIE.2009.2036026},
file = {::},
isbn = {0278-0046},
issn = {02780046},
journal = {IEEE Transactions on Industrial Electronics},
keywords = {Control of drive,Converter control,Digital signal processor (DSP),Multiphase drive,Reliability},
number = {2},
pages = {575--583},
title = {{Multiphase power converter drive for fault-tolerant machine development in aerospace applications}},
volume = {57},
