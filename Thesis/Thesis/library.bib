keywords = {Admittance compensation,Fuel-cell power conditioning system,LCL filter,Proportional-resonant compensator,Single-phase grid-tie inverter},
number = {4},
pages = {1716--1723},
title = {{Admittance compensation in current loop control for a grid-tie LCL fuel cell inverter}},
volume = {23},
year = {2008}
}
