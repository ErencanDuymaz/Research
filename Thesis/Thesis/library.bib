keywords = {Permanent-magnet synchronous motor (PMSM) drives,predictive direct torque control (DTC),ripple reduction,steady-state performance,transient-state performance},
number = {6},
pages = {3876--3888},
title = {{Minimizing Torque and Flux Ripples and Improving Dynamic Response of PMSM Using a Voltage Vector with Optimal Parameters}},
volume = {63},
year = {2016}
}
