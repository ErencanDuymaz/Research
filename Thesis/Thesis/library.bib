year = {2015}
}
@article{Han2017,
abstract = {Microgrids consist of multiple parallel-connected distributed generation$\backslash$n(DG) units with coordinated control strategies, which are able to$\backslash$noperate in both grid-connected and islanded modes. Microgrids are$\backslash$nattracting considerable attention since they can alleviate the stress of$\backslash$nmain transmission systems, reduce feeder losses, and improve system$\backslash$npower quality. When the islanded microgrids are concerned, it is$\backslash$nimportant to maintain system stability and achieve load power sharing$\backslash$namong the multiple parallel-connected DG units. However, the poor active$\backslash$nand reactive power sharing problems due to the influence of impedance$\backslash$nmismatch of the DG feeders and the different ratings of the DG units are$\backslash$ninevitable when the conventional droop control scheme is adopted.$\backslash$nTherefore, the adaptive/improved droop control, network-based control$\backslash$nmethods, and cost-based droop schemes are compared and summarized in$\backslash$nthis paper for active power sharing. Moreover, non-linear and unbalanced$\backslash$nloads could further affect the reactive power sharing when regulating$\backslash$nthe active power, and it is difficult to share the reactive power$\backslash$naccurately only by using the enhanced virtual impedance method.$\backslash$nTherefore, the hierarchical control strategies are utilized as$\backslash$nsupplements of the conventional droop controls and virtual impedance$\backslash$nmethods. The improved hierarchical control approaches such as the$\backslash$nalgorithms based on graph theory, multi-agent system, the gain$\backslash$nscheduling method, and predictive control have been proposed to achieve$\backslash$nproper reactive power sharing for islanded microgrids and eliminate the$\backslash$neffect of the communication delays on hierarchical control. Finally, the$\backslash$nfuture research trends on islanded microgrids are also discussed in this$\backslash$npaper.},
