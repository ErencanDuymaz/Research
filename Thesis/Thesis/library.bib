journal = {Renewable and Sustainable Energy Reviews},
keywords = {Deloading methods,Grid codes,Primary frequency control,Synthetic inertia,Wind power plants},
pages = {551--564},
title = {{Participation of wind power plants in system frequency control: Review of grid code requirements and control methods}},
volume = {34},
year = {2014}
