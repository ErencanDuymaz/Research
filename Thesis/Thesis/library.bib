author = {Drobyshevski, E. M. and Rozov, S. I. and Zhukov, B. G. and Kurakin, R. O. and Beloborodyy, M. V. and Latypov, V. G.},
doi = {10.1109/20.911792},
file = {::},
isbn = {0018-9464},
issn = {00189464},
journal = {IEEE Transactions on Magnetics},
keywords = {3D MHD,Arc mode,Pinch instability,Shunting arcs},
number = {1},
pages = {62--66},
title = {{Physics of solid armature launch transition into arc mode}},
volume = {37},
year = {2001}
