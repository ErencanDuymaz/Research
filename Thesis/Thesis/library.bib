journal = {IEEE Transactions on Power Electronics},
keywords = {Circulating energy,high efficiency,high-frequency transformer,input-series-output-parallel (ISOP),isolated bidirectional dc-dc converter,solid-state transformer (SST),zero voltage switching (ZVS)},
number = {12},
pages = {3599--3608},
title = {{High-frequency transformer isolated bidirectional DC-DC converter modules with high efficiency over wide load range for 20 kVA solid-state transformer}},
volume = {26},
