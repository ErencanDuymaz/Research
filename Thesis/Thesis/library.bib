journal = {16th International Power Electronics and Motion Control Conference and Exposition, PEMC 2014},
keywords = {Front-loading,Laminated,Parasitic inductance,Printed circuit board},
pages = {569--574},
title = {{Pattern design criteria of main circuit using printed circuit boards for parasitic inductance reduction}},
year = {2014}
}
