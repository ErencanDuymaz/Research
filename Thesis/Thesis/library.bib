journal = {IEEE Transactions on Plasma Science},
keywords = {Countercurrent commutation,XRAM generator,current multiplication,semiconductor opening switch},
number = {1 PART 1},
pages = {263--267},
title = {{Twenty-stage toroidal XRAM generator switched by countercurrent thyristors}},
volume = {39},
year = {2011}
}
