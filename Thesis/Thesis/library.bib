isbn = {0960-1481},
issn = {09601481},
journal = {Renewable Energy},
keywords = {Digital control,FPGA,Grid-connected PV system,Power factor control,Single-phase inverter},
number = {1},
pages = {315--321},
title = {{Digital power factor control and reactive power regulation for grid-connected photovoltaic inverter}},
volume = {34},
year = {2009}
