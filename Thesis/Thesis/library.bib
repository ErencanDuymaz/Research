@article{Wang2016c,
abstract = {This paper presents model predictive voltage control (MPVC) and model predictive current control (MPCC) methods for the single-inductor multiple-output (SIMO) DC-DC converter. The proposed MPVC and MPCC methods are able to solve the cross regulation problem, which is a critical issue in SIMO DC-DC converters. The design of the proposed methods using augmented state-space models, cost functions and constraints for SIMO DC-DC converter is discussed. Simulation of the influence of control horizon N C and Lagrange multiplier $\lambda$ of the MPVC and MPCC methods is conducted to guide the control parameter setting for hardware implementation. The transient responses to reference variation of the two proposed methods are also presented. Moreover, a single-inductor dual-output (SIDO) buck converter hardware prototype is developed and the proposed control methods are implemented. Four test cases are conducted based on this prototype. Simulation and experimental results show that the two proposed methods guarantee low cross regulation for SIMO DC-DC converters and have a fast response speed to reference variations.},
annote = {A Model predictive control application.},
author = {Wang, Benfei and Kanamarlapudi, Venkata Ravi Kishore and Xian, Liang and Peng, Xiaoyang and Tan, Kuan Tak and So, Ping Lam},
