@article{Butz1986,
abstract = {Battelle's Columbus Division recently completed a Small Caliber Electromagnetic Launcher (EML) Engineering Feasibility Study Program under funding from the U.S. Army Armament Research and Development Center Joint Services Small Arms Program. Included in the study program was a preliminary feasibility and conceptual design study of a novel, hybrid EML with a pulsed, magnetohydrodynamic (MHD) generator power supply. This concept was evolved by Battelle under an earlier Joint Services Small Arms Program Office effort. The system to be designed was a 0.30 caliber, tactical, portable EML capable of accelerating a 2.5 g projectile to near 1500 m/s, using a 1 m railgun and a solid armature. A first-order performance model for the integrated EML and generator was developed and implemented on a personal computer. Trade studies were conducted on numerous parameters of interest, and required propellant and other characteristics were specified. Results of the model development and trade studies are presented herein, and they indicate that significant technology advances are required to weaponize this EML concept. For the purposes of conducting the necessary para- metric studies, a baseline launcher configuration was specified. A 1 m long generator and a 1 m long railgun were selected, with an average generator channel diameter of 25.4 mm, and a railgun bore diameter of 7.62 mm. Rails and electrodes were taken to be copper for all cases. The downstream 50{\%} of the MHD channel was made active, with the magnetic field provided by external, battery-powered saddle coils. The projectile was initially accelerated by a pressure sufficient that 200 m/s velocity was reached by the time the current is first applied, The baseline generator characteristics were taken as: a 5 T magnetic flux density, a 2000 m/s plasma velocity, and an effective plasma conductivity of 2000 mho/m. With such a generator, a projectile muzzle velocity of 1520 m/s is predicted.},
author = {Butz, Donald J and Levin, Victor},
file = {::},
journal = {Ieee Transactions on Magnetics},
number = {6},
pages = {1416--1422},
title = {{1416 Ieee Transactions on Magnetics, Vol. Mag-22, No. 6, November 1986}},
volume = {MAG 22},
year = {1986}
}
