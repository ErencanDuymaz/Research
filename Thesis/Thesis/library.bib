@article{Stefani2001a,
abstract = {An important research problem for solid armature railguns is$\backslash$nunderstanding the sudden transition from low-voltage sliding electrical$\backslash$ncontact between the rails and armature to high-voltage contact involving$\backslash$na plasma arc. Although an armature can fail in many ways and thereby$\backslash$n{\&}ldquo;transition,{\&}rdquo; two distinct transition mechanisms currently$\backslash$nlimit the performance of solid armature electromagnetic (EM) launchers.$\backslash$nOne mechanism, broadly termed {\&}ldquo;wear-induced transition,{\&}rdquo;$\backslash$nresults from uneven or excessive loss of material from the contact$\backslash$ninterface. The other mechanism is associated with a rapid reduction in$\backslash$ndriving current (negative dI/dt). A series of experiments have shown the$\backslash$nwaveform-induced transition mechanism is independent of the state of$\backslash$nwear of an armature. This paper explores this mechanism, which we call$\backslash$n{\&}ldquo;electrodynamic transition,{\&}rdquo; and seek to explain the cause$\backslash$nof transition as an electromechanical instability that disrupts the$\backslash$nliquid film interface between armature and rail. Using 3-D finite$\backslash$nelement analyses (FEA), we observed the development of localized forces$\backslash$nat the edges of the armature as the driving current drops rapidly. This$\backslash$nbehavior, we believe, may cause molten material to be ejected from the$\backslash$narmature-rail contact region},
author = {Stefani, Francis and Levinson, Scott and Satapathy, Sikhanda and Parker, Jerald},
doi = {10.1109/20.911800},
file = {::},
issn = {00189464},
journal = {IEEE Transactions on Magnetics},
keywords = {Arcing contact,Armature,Electromagnetic launcher,Railgun,Sliding electric contact,Transition},
number = {1 I},
pages = {101--105},
title = {{Electrodynamic transition in solid armature railguns}},
volume = {37},
year = {2001}
}
@article{Fornasiero2010,
abstract = {Fractional-slot PM machines have several advantages, but they suffer from the high contents of space harmonics in the air-gap MMF distribution. The MMF space harmonic amplitude and frequency depend on the particular combination of number of slots and poles. Such harmonics induce losses in the rotor since they are not synchronous with the rotor speed. This paper compares two kind of machines with the same stator lamination but with different number of phases, a three phase and a five-phase PM machine. Several winding configurations are compared, changing the number of poles but maintaining the same number of slots. Although in each case there is a reduction in terms of harmonic contents, it is shown that rotor losses reduction is lower than expected. Some considerations about the harmonic impact are given.},
author = {Fornasiero, Emanuele and Bianchi, Nicola and Bolognani, Silverio},
doi = {10.1109/ICELMACH.2010.5607888},
file = {::},
isbn = {9781424441754},
journal = {19th International Conference on Electrical Machines, ICEM 2010},
keywords = {Multiphase machines,PM machines,Rotor losses,Variable speed drives},
number = {6},
pages = {2338--2345},
title = {{Rotor losses in fractional-slot three-phase and five-phase PM machines}},
volume = {46},
year = {2010}
}
